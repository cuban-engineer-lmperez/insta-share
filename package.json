{
  "name": "insta-share",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve -o --watch -c development",
    "start:docker": "ng serve --host=0.0.0.0 --poll=2000 --watch -c development",
    "build": "ng build",
    "build:prod": "ng build --c=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:cov": "ng test --no-watch --coverage --silent",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "firebase:deploy": "ng b --c=production && firebase deploy --only hosting"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.6",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.6",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "firebase": "^10.13.2",
    "insta-share": "file:",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-builders/jest": "^18.0.0",
    "@angular-devkit/build-angular": "^18.2.5",
    "@angular/cli": "^18.2.6",
    "@angular/compiler-cli": "^18.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.13",
    "angular-eslint": "18.3.1",
    "eslint": "^9.9.1",
    "jasmine-core": "~5.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.2.0"
  }
}
